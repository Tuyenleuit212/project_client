{
   "version": 2,
   "builds": [
       {
           "src": "server.js",
           "use": "@vercel/node"
       },
       {
           "src": "public/image",
           "use": "@vercel/static"
       }
   ],
   "routes":[
       {
           "src": "/(.*)",
           "dest": "server.js"
       },
       {
           "src": "/",
           "dest": "public/index.html"
       },
       {
           "src": "/public/(.*)",
           "dest": "public/$1"
       }
   ]
}
